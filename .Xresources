Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

URxvt.font: xft:hack:bold:size=12

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt.depth: 32
URxvt.transparent: false
URxvt.fading: 0
URxvt.loginShell: true
URxvt.saveLines: 20000
URxvt.internalBorder: 3
URxvt.lineSpace: 0

URxvt*background: #1b1d1e
URxvt*foreground: #d0d0d0
URxvt*cursorColor: #ff00ff
URxvt*color0: #1b1d1e
URxvt*color1: #ff0044
URxvt*color2: #82b414
URxvt*color3: #fd971f
URxvt*color4: #1080d0
URxvt*color5: #7e40a5
URxvt*color6: #3579a8
URxvt*color7: #9999aa
URxvt*color8: #266c98
URxvt*color9: #ff0090
URxvt*color10: #82b414
URxvt*color11: #ffaa00
URxvt*color12: #7070f0
URxvt*color13: #d63ae1
URxvt*color14: #66d9ef
URxvt*color15: #d0d0d0

! man pages
URxvt*.colorIT: #87af5f
URxvt*.colorBD: #ffffff
URxvt*.colorUL: #ffffff
URxvt.iconFile: /usr/share/icons/hicolor/scalable/apps/Terminal.svg

! urxt plugins
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select,selection-to-clipboard,pasta

URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/firefox
URxvt.underlineURLs: True
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V: perl:pasta:paste
URxvt.clipboard.autocopy: true

